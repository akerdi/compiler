{
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "args": [
        "${workspaceRoot}/aa.js -w standalone"
      ],
      "runtimeExecutable": "npm",
      "runtimeArgs": [
          "run",
          "dev",
      ],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "protocol": "inspector"
    }
  ]
}